author = {Esli Osmanlliu and Senthujan Senkaiahliyan and Alex Eisen-Cuadra and Mahima Kalla and Nai Lee Kalema and Alice Rangel Teixeira and Leo Celi },
title = {The Urgency of Environmentally Sustainable and Socially Just Deployment of Artificial Intelligence in Health Care},
