  volume   =  13,
  pages    = "e58712",
  month    =  may,
  year     =  2025,
  keywords = "accessibility; dermatological; dermatological care;
              dermatologist; dermatology; prison health; remote consultation;
              teledermatology; telehealth; telemedicine; telephone
              consultation; video consultation; virtual health; virtual
              medicine",
  language = "en"
}
@ARTICLE{Loo2025-ye,
