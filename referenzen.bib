@article{10.1001/jama.2025.0946,
    author = {Mandl, Kenneth D.},
    title = {How AI Could Reshape Health Care—Rise in Direct-to-Consumer Models},
    journal = {JAMA},
    year = {2025},
    month = {02},
    abstract = {A time may be approaching when wise patients demand artificial intelligence (AI)–informed care as a standard of quality. How this expectation will be met remains unclear; indeed, AI may put the health system at a crossroads. Even at this formative stage, and despite limitations, predictive AI and large language models (LLMs) show potential for accurately diagnosing and prognosticating across domains, including medicine, pathology, and radiology.Yet traditional health care organizations (HCOs) grapple with the complexities of AI, from integrating it into rigid electronic health record (EHR) workflows to ensuring fairness and monitoring performance. HCOs are often constrained in innovation by entrenched operational structures. Deploying AI demands rigorous oversight, including surveillance for data drift and shift (ie, changes in data patterns that can degrade model performance), while promoting fairness, transparency, and avoidance of bias. Meanwhile, an emerging direct-to-consumer (DTC) health care market bypasses traditional HCOs. For example, Amazon integrates telehealth, primary care, and pharmacy services into its Prime membership. Additionally, Big Pharma host consumer-facing portals streamlining access to medications and integrating telehealth consultations.},
    issn = {0098-7484},
    doi = {10.1001/jama.2025.0946},
    url = {https://doi.org/10.1001/jama.2025.0946},
