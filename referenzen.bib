@article{HEITMANN2003195,
  title = {Discharge and referral data exchange using global standards—the SCIPHOX project in Germany},
  journal = {International Journal of Medical Informatics},
  volume = {70},
  number = {2},
  pages = {195-203},
  year = {2003},
